<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.derik.rxjavademo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.SimpleRxJavaActivity" />
        <activity android:name=".view.MapRxJavaActivity" />
        <activity android:name=".view.FlatMapRxJavaActivity" />
        <activity android:name=".view.TakeRxJavaActivity" />
        <activity android:name=".view.DisposableRxJavaActivity" />
        <activity android:name=".view.TimerRxJavaActivity" />
        <activity android:name=".view.DelayRxJavaActivity" />
        <activity android:name=".view.IntervalRxJavaActivity" />
        <activity android:name=".view.SingleRxJavaActivity" />
        <activity android:name=".view.CompletableRxJavaActivity" />
        <activity android:name=".view.FlowableRxJavaActivity" />
        <activity android:name=".view.MaybeRxJavaActivity" />
        <activity android:name=".view.BufferRxJavaActivity" />
        <activity android:name=".view.FilterRxJavaActivity" />
        <activity android:name=".view.SkipRxJavaActivity" />
        <activity android:name=".view.ScanRxJavaActivity" />
        <activity android:name=".view.ReplayRxJavaActivity" />
    </application>

</manifest>